{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "module": "es2020",
    "esModuleInterop": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom",
      "ES2021.String"
    ],
    "resolveJsonModule": true,
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": false,
    "enableIvy": true
  }
}